{
  "projectType": "library",
  "sourceRoot": "libs/oidc-api-angular/src",
  "prefix": "z-auth",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "dist/libs/oidc-api-angular"
      ],
      "options": {
        "project": "libs/oidc-api-angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/oidc-api-angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/oidc-api-angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/oidc-api-angular"
      ],
      "options": {
        "jestConfig": "libs/oidc-api-angular/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/oidc-api-angular/**/*.ts",
          "libs/oidc-api-angular/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
